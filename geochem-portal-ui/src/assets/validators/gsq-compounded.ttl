<https://w3id.org/geochem/1.0/analyticalmethod/ramanspectrometry>
    a skos:Concept ;
    dcterms:source "https://iupac.org/wp-content/uploads/2019/10/PAC-REC-19-02-03.R2_PR191002MC.pdf"^^xsd:anyURI ;
